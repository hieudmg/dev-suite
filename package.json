    "plasmo": "^0.86.3",
    "react": "^18.3.1",
    "react-dom": "18.3.1",
    "urlpattern-polyfill": "^10.0.0",
    "webextension-polyfill": "^0.12.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.2.1",
    "@types/chrome": "0.0.268",
    "@types/lodash": "^4.17.4",
    "@types/node": "20.12.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/webextension-polyfill": "^0.10.7",
    "prettier": "^3.2.5",
    "svgo": "^3.3.2",
    "typescript": "^5.4.5",
    "web-ext": "^7.11.0"
