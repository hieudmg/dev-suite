{
  "name": "dev-suite",
  "displayName": "Dev suite",
  "version": "0.0.1",
  "description": "Dev Suite",
  "license": "MIT",
  "author": "Hieudmg",
  "scripts": {
    "dev": "plasmo dev",
    "dev:firefox": "plasmo dev --target=firefox-mv2",
